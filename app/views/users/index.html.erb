<ul>
  <% @users.each do |user| %>
    <li><%= user.first_name + " " + user.last_name + ": " + user.username%></li>
        <ul>
        <strong>Member of:</strong>
        <% user.teams.each do |team| %>
            <li><%= team.name %></li>
        <% end %>
        <strong>Coach of:</strong>
        <% user.coaching_teams.each do |team| %>
            <li><%= team.name %></li>
        <% end %>
        </ul>
  <% end %>
</ul>